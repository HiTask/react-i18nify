{
  "name": "react-i18nify",
  "version": "2.0.0-9",
  "description": "Simple i18n translation and localization components and helpers for React applications.",
  "main": "./build/index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src/**/*.jsx src/**/*.js __test__/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "rm -rf build && babel src -d build",
    "prepare": "npm run build",
    "precommit": "npm run lint && npm test"
  },
  "files": [
    "build",
    "src",
    "example"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JSxMachina/react-i18nify.git"
  },
  "author": "JSxMachina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JSxMachina/react-i18nify/issues"
  },
  "homepage": "https://github.com/JSxMachina/react-i18nify#readme",
  "keywords": [
    "react",
    "i18n",
    "translation",
    "localization",
    "components",
    "helpers",
    "javascript",
    "flux",
    "redux"
  ],
  "runkitExampleFilename": "example/example.js",
  "dependencies": {
    "date-fns": "^2.0.0-alpha.26",
    "intl": "^1.2",
    "prop-types": "^15.6.2",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "^1.2.0",
    "jest": "23.6.0",
    "react": "^16.6.3",
    "react-dom": "16.6.3"
  }
}
